<portlet-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
    xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
    version="2.0">

    <portlet>
        <portlet-name>BusinessEmailAdmin</portlet-name>
        <portlet-class>org.jasig.springframework.web.portlet.context.ContribDispatcherPortlet</portlet-class>
        <init-param>
            <name>contextConfigLocation</name>
            <value>/WEB-INF/context/portlet/business-email-admin.xml</value>
        </init-param>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
        </supports>
        <portlet-info>
            <title>Campus Business Email Admin</title>
            <short-title>Campus Business Email Admin</short-title>
        </portlet-info>
        <portlet-preferences>
            <preference>
              <name>emplidUserAttributes</name>
              <value>eduWisconsinHRSEmplID</value>
              <value>eduWisconsinHRPersonID</value>
              <value>wiscEduHRSEmplid</value>
              <value>wisceduhrpersonid</value>
              <value>legacywisceduhrpersonid</value>
            </preference>
        </portlet-preferences>
    </portlet>

    <portlet>
        <portlet-name>PayrollInformation</portlet-name>
        <portlet-class>org.jasig.springframework.web.portlet.context.ContribDispatcherPortlet</portlet-class>
        <init-param>
            <name>contextConfigLocation</name>
            <value>/WEB-INF/context/portlet/payroll-information.xml</value>
        </init-param>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
        </supports>
        <portlet-info>
            <title>Payroll Information</title>
            <short-title>Payroll Information</short-title>
        </portlet-info>
        <portlet-preferences>
            <preference>
              <name>emplidUserAttributes</name>
              <value>eduWisconsinHRSEmplID</value>
              <value>eduWisconsinHRPersonID</value>
              <value>wiscEduHRSEmplid</value>
              <value>wisceduhrpersonid</value>
              <value>legacywisceduhrpersonid</value>
            </preference>
        </portlet-preferences>
    </portlet>

    <portlet>
        <portlet-name>BenefitInformation</portlet-name>
        <portlet-class>org.jasig.springframework.web.portlet.context.ContribDispatcherPortlet</portlet-class>
        <init-param>
            <name>contextConfigLocation</name>
            <value>/WEB-INF/context/portlet/benefit-information.xml</value>
        </init-param>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
        </supports>
        <portlet-info>
            <title>Benefit Information</title>
            <short-title>Benefit Information</short-title>
        </portlet-info>
        <portlet-preferences>
            <preference>
              <name>emplidUserAttributes</name>
              <value>eduWisconsinHRSEmplID</value>
              <value>eduWisconsinHRPersonID</value>
              <value>wiscEduHRSEmplid</value>
              <value>wisceduhrpersonid</value>
              <value>legacywisceduhrpersonid</value>
            </preference>
        </portlet-preferences>
    </portlet>

    <portlet>
        <portlet-name>TimeAndAbsence</portlet-name>
        <portlet-class>org.jasig.springframework.web.portlet.context.ContribDispatcherPortlet</portlet-class>
        <init-param>
            <name>contextConfigLocation</name>
            <value>/WEB-INF/context/portlet/time-absence.xml</value>
        </init-param>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
        </supports>
        <portlet-info>
            <title>Time and Absence</title>
            <short-title>Time and Absence</short-title>
        </portlet-info>
        <portlet-preferences>
            <preference>
              <name>emplidUserAttributes</name>
              <value>eduWisconsinHRSEmplID</value>
              <value>eduWisconsinHRPersonID</value>
              <value>wiscEduHRSEmplid</value>
              <value>wisceduhrpersonid</value>
              <value>legacywisceduhrpersonid</value>
            </preference>
        </portlet-preferences>
    </portlet>

    <portlet>
        <portlet-name>ContactInfo</portlet-name>
        <portlet-class>org.jasig.springframework.web.portlet.context.ContribDispatcherPortlet</portlet-class>
        <init-param>
            <name>contextConfigLocation</name>
            <value>/WEB-INF/context/portlet/contact-info.xml</value>
        </init-param>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
        </supports>
        <portlet-info>
            <title>Contact Information</title>
            <short-title>Contact Information</short-title>
        </portlet-info>
        <portlet-preferences>
            <preference>
              <name>businessEmailRoles</name>
              <value>pags.uw-madison</value>
              <value>pags.uw-sys-madison</value>
            </preference>
            <preference>
              <name>emplidUserAttributes</name>
              <value>eduWisconsinHRSEmplID</value>
              <value>eduWisconsinHRPersonID</value>
              <value>wiscEduHRSEmplid</value>
              <value>wisceduhrpersonid</value>
              <value>legacywisceduhrpersonid</value>
            </preference>
            <preference>
              <name>displayName</name>
              <value>displayName</value>
            </preference>
            <preference>
            	<name>pviUserAttributes</name>
            	<value>wiscedupvi</value>
            </preference>
        </portlet-preferences>
    </portlet>

    <portlet>
        <portlet-name>ManagerTimeApproval</portlet-name>
        <portlet-class>org.jasig.springframework.web.portlet.context.ContribDispatcherPortlet</portlet-class>
        <init-param>
            <name>contextConfigLocation</name>
            <value>/WEB-INF/context/portlet/manager-time-approval.xml</value>
        </init-param>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
        </supports>
        <portlet-info>
            <title>Manager Time and Approval</title>
            <short-title>Manager Time and Approval</short-title>
        </portlet-info>
        <portlet-preferences>
            <preference>
              <name>emplidUserAttributes</name>
              <value>eduWisconsinHRSEmplID</value>
              <value>eduWisconsinHRPersonID</value>
              <value>wiscEduHRSEmplid</value>
              <value>wisceduhrpersonid</value>
              <value>legacywisceduhrpersonid</value>
            </preference>
        </portlet-preferences>
    </portlet>
    
    <user-attribute>
        <name>eduWisconsinHRSEmplID</name>
    </user-attribute>
    <user-attribute>
        <name>eduWisconsinHRPersonID</name>
    </user-attribute>
    <user-attribute>
        <name>wiscEduHRSEmplid</name>
    </user-attribute>
    <user-attribute>
        <name>wisceduhrpersonid</name>
    </user-attribute>
    <user-attribute>
        <name>legacywisceduhrpersonid</name>
    </user-attribute>
    <user-attribute>
        <name>displayName</name>
    </user-attribute>
    <user-attribute>
    	<name>wiscedupvi</name>
    </user-attribute>
    <user-attribute>
    	<name>wiscedupreferredname</name>
    </user-attribute>
    <user-attribute>
    	<name>wiscedupreferredmiddlename</name>
    </user-attribute>
    <user-attribute>
    	<name>wiscedupreferredfirstname</name>
    </user-attribute>
    
    <filter>
        <filter-name>springSecurityPortletFilterChain</filter-name>
        <filter-class>org.jasig.springframework.web.portlet.filter.DelegatingPortletFilterProxy</filter-class>
        <lifecycle>ACTION_PHASE</lifecycle>
        <lifecycle>EVENT_PHASE</lifecycle>
        <lifecycle>RENDER_PHASE</lifecycle>
        <lifecycle>RESOURCE_PHASE</lifecycle>
    </filter>
    
    <filter-mapping>
        <filter-name>springSecurityPortletFilterChain</filter-name>
        <portlet-name>*</portlet-name>
    </filter-mapping>
</portlet-app>